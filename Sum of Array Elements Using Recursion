#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int sumArray(int arr[], int num){
     if(num==0)
         return 0;
    
    return arr[num-1] + sumArray(arr, num-1);
}

int main() {
    int num;
    scanf("%d",&num);
    int arr[num];
    for(int i=0;i<num;i++){
        scanf("%d",&arr[i]);
    }
    
    int sum=sumArray(arr, num);
    
    printf("%d",sum);
    return 0;
}
