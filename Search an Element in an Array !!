#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x;
    scanf("%d",&x);
    int arr[x];
        for(int i=0;i<x;i++){
        scanf("%d",&arr[i]);
        }
    int N;
    scanf("%d",&N);
    int found=0;
    for(int i=0;i<x;i++){
        if(arr[i]==N){
            printf("Element found at position %d",i+1);
            found=1;
            break;
        }
    }
    if(!found){
        printf("Element not found");
    }
    return 0;
}
