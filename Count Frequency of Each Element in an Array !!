#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num;
    scanf("%d",&num);
    
    int arr[num];
    int freq[num];
    int visited[num];

    for(int i=0;i<num;i++){
        scanf("%d",&arr[i]);
        freq[i]=1;
        visited[i]=0;
    }
    
    for(int i=0;i<=100;i++){
        if(visited[i]){
            continue;
        }
     for(int j=i+1;j<num;j++){
        if(arr[i]==arr[j]){
            freq[i]++;
            visited[j]=1;
        }
     }
    }
    for(int i=0;i<num;i++){
        if(!visited[i]){
            printf("%d: %d\n",arr[i],freq[i]);
        }
    }
    
    return 0;
}
