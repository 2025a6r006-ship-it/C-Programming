#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char S1[1000], rS2[1000];
    scanf("%[^\n]",S1);
    for(int i=0;S1[i]!='\0';i++){
        S1[i] = tolower(S1[i]);
    }
    int k=0;
    for(int i=strlen(S1)-1;i>=0;i--){
        rS2[k++] = S1[i];
    }
    rS2[k]='\0';
    if(strcmp(S1,rS2)==0){
        printf("Palindrome");
    }
    else{
        printf("Not Palindrome");
    }
    return 0;
}
