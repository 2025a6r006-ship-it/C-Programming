//WAP to obtain the factorial of the numbers among the 1 to N numbers

#include<stdio.h>
int main (){
    int N,i,fact;
    printf("Enter N: ");
    scanf("%d",&N);
    for(i=1;i<=N;i++){
        fact=1;
        for(int j=1;j<=i;j++){
            fact = fact*j;
    }
    printf("Factorial of %d is %d\n", i,fact);
}
    return 0;
}
