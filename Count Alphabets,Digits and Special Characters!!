#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char S[1000];
    int alphacount=0,digitcount=0,spcount=0;
    scanf("%[^\n]%*c",S);
    for(int i=0;i<S[i];i++){
        if(isalpha(S[i])){
            alphacount++;
        }
        else if(isdigit(S[i])){
            digitcount++;
        }
        else{
            spcount++;
        }
    }
    printf("%d %d %d",alphacount,digitcount,spcount);
    return 0;
}
