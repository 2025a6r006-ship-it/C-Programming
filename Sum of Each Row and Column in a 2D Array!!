#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int r,c;
    scanf("%d%d",&r,&c);
    int a[r][c];
    for(int i=0;i<r;i++){
        for(int j=0;j<c;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(int i=0;i<r;i++){
        int rsum=0;
        for(int j=0;j<c;j++){
            rsum=rsum+a[i][j];
        }
        printf("%d\n",rsum);
    }
    
    for(int j=0;j<c;j++){
        int csum=0;
        for(int i=0;i<r;i++){
            csum=csum+a[i][j];
        }
        printf("%d\n",csum);
    }
    return 0;
}
