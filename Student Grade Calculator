#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int Ph,Ch,Bi,Mt,Com,Total;
    float Percentage;
    
    scanf("%d%d%d%d%d", &Ph,&Ch,&Bi,&Mt,&Com);
    Total = Ph+Ch+Bi+Mt+Com;
    
    Percentage = Total/5.0;
    printf("Percentage = %.2f\n", Percentage);
    
        if(Percentage>=90){
            printf("Grade = A");
        }
        else if(Percentage>=80){
            printf("Grade = B");
            }
        else if(Percentage>=70){
            printf("Grade = C");
            }
        else if(Percentage>=60){
            printf("Grade = D");
            }
        else if(Percentage>=40){
            printf("Grade = E");
            }
        else{
            printf("Grade = F");
            }
        
    return 0;
}
