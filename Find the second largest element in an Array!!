#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <limits.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int max,secmax;
    max=INT_MIN;
    secmax=INT_MIN;
    for(int i=0;i<n;i++){
        if(a[i]>max){
            secmax=max;
            max=a[i];
        }
        else if(a[i]>secmax && a[i]<max){
            secmax=a[i];
        }
    }
    if(secmax==INT_MIN){
        printf("No second largest element");
    }
    else if(secmax!=INT_MIN){
        printf("%d",secmax);
    }
    return 0;
}
