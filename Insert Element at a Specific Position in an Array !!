#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[101];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int element;
    scanf("%d",&element);
    
    int index;
    scanf("%d",&index);
    
    if(index==0){
        index=1;
    }
    
    for(int i=n;i>=index;i--){
        a[i]=a[i-1];
    }
    a[index-1]=element;
    n++;
    
    for(int i=0;i<n;i++){
        if(i>0){
            printf(" ");
        }
        printf("%d",a[i]);
    }
    return 0;
}
