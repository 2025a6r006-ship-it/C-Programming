#include<stdio.h>
int main (){
    int i,max,N,num;
    max = 0;
    printf("Enter N = ");
    scanf("%d", &N);
    for(i=1;i<=N;i++){
        printf("Enter number = ");
        scanf("%d", &num);
        if(num > max)
        max = num;
    } 
    printf("Largest Number = %d ", max);
        
return 0;
}
