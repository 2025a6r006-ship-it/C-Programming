#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>

int main() {
    char S[1000];
    scanf("%[^\n]",S);
    int count=0;
    for(int i=0;i<S[i];i++){
        char ch = toupper(S[i]);
        if(ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
            count++;
    }
    printf("%d",count);
    
    return 0;
}
