#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int num,count=0;
    scanf("%d",&num);
    for(int i=0;i<n;i++){
        if(a[i]==num){
            count++;
        }
    }
    printf("%d",count);
    return 0;
}
